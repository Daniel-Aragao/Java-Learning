package Exercicing;

import java.awt.BorderLayout;

import javax.swing.JPanel;
import javax.swing.JTextArea;

public class TextArea extends JPanel{
	JTextArea textArea;

	public TextArea() {
		textArea = new JTextArea();
		setLayout(new BorderLayout());
		
		add(textArea,BorderLayout.CENTER);
	}
	
	
}
